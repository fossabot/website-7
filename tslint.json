{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts", "**/*.js"]
  },
  "rules": {
    "jsx-no-lambda": false,
    "no-console": false,
    "ordered-imports": false
  }
}
