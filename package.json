{
  "name": "paderbornjs-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version react-app-rewired start",
    "build": "REACT_APP_VERSION=$npm_package_version react-app-rewired build",
    "test": "react-app-rewired test",
    "lint": "tslint -t verbose --project .",
    "precommit": "lint-staged",
    "ci:build": "CI=true yarn run build",
    "ci:lint": "CI=true yarn run lint",
    "ci:test": "CI=true react-app-rewired test --runInBand --silent --colors && codecov -t $CODECOV_TOKEN"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "apollo-cache-inmemory": "^1.3.5",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.3",
    "apollo-link-batch-http": "^1.2.3",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "emotion": "^9.2.12",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-scripts": "2.0.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.5",
    "@types/reach__router": "^1.2.0",
    "@types/react": "^16.4.17",
    "@types/react-dom": "^16.0.9",
    "codecov": "^3.1.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.0",
    "react-app-rewire-emotion": "^4.0.0",
    "react-app-rewire-typescript-babel-preset": "^3.0.0",
    "react-app-rewired": "^1.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
